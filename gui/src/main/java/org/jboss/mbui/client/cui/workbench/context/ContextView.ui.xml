<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style>
        .context {
            background-color: white;
            border: 1px solid rgb(167, 171, 180);
            border-left: none;
            padding: 10px;
            height: 100%;
        }

        .context p {
            margin: 0;
        }
    </ui:style>

    <g:HTMLPanel addStyleNames="{style.context}">
        <p>ContextView not yet implemented</p>
    </g:HTMLPanel>

</ui:UiBinder>